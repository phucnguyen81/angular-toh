    "prod:build": "ng build --configuration production",
    "prod:deploy": "ng deploy",
    "lint": "ng lint",
    "prepare": "husky install",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
