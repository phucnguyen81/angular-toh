{
  "name": "angular-toh",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "api": "json-server --watch db.json --configuration development --host 0 --port 3000",
    "start": "ng serve --host 0 --port 4200 --configuration development",
    "test": "ng test --configuration development --browsers ChromeHeadless --watch",
    "build": "ng build --watch --configuration development",
    "ci:test": "ng test --configuration development --browsers ChromeHeadless --watch=false",
