    "husky": "^8.0.0",
    "jasmine-core": "~4.6.0",
    "json-serve": "^0.1.0",
    "json-server": "^0.17.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
